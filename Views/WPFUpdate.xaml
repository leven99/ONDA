<Window x:Class="SocketDA.Views.WPFUpdate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SocketDA.Views"
        mc:Ignorable="d"
        Title="SocketDA" WindowStartupLocation="CenterScreen" Style="{DynamicResource UpdateWindowChromeStyle}">
    
    <!--Window Style-->
    <Window.Resources>
        <Style x:Key="UpdateWindowChromeStyle" TargetType="{x:Type local:WPFUpdate}">
            <Setter Property="WindowChrome.WindowChrome">
                <Setter.Value>
                    <WindowChrome/>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>

    <!--Root-->
    <DockPanel x:Name="RootDockPanel">
        <Grid x:Name="RootGrid">
            
        </Grid>
    </DockPanel>
</Window>
